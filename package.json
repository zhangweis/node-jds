{
  "name": "bitcoinjs",
  "version": "0.2.8",
  "description": "Implementation of Bitcoin's peer-to-peer layer for Node.js",
  "main": "./lib/bitcoin",
  "keywords": [
    "peer-to-peer",
    "bitcoin",
    "client"
  ],
  "bin": {
    "bitcoinjs": "./bin/bitcoinjs"
  },
  "scripts": {
    "install": "node-gyp configure build",
    "test": "vows --spec test/*.js"
  },
  "engines": {
    "node": ">= 0.9.4"
  },
  "man": [
    "./man/bitcoinjs.1",
    "./man/run.1",
    "./man/db-reset.1",
    "./man/db-drop.1",
    "./man/test.1"
  ],
  "dependencies": {
    "bignum": ">=0.4.0",
    "binary": ">=0.2.5 <0.3",
    "bs58": "^2.0.1",
    "buffers": ">=0.1.1",
    "buffertools": ">=1.x",
    "colors": ">=0.5.0",
    "crypto-hashing": "^0.3.1",
    "ecdsa": "^0.6.0",
    "forever": ">=0.6.1",
    "irc": ">=0.2.1",
    "jsonrpc2": ">=0.1.0",
    "leveldown": ">=0.2.1",
    "lru-cache": ">=1.0.4",
    "mkdirp": ">=0.2.1",
    "npm": "*",
    "pkginfo": ">=0.2.0",
    "progress-bar": ">=0.1.1",
    "step": ">=0.0.4",
    "winston": ">=0.2.6",
    "yanop": ">=0.1.1"
  },
  "devDependencies": {
    "vows": ">=0.5.8",
    "ronn": ">=0.3.6"
  },
  "author": "Stefan Thomas <justmoon@members.fsf.org> (http://www.justmoon.net)",
  "contributors": [
    "Andrew Schaaf <andrew@andrewschaaf.com>",
    "Mike Hearn <hearn@google.com>",
    "Robert Kieffer <robert@broofa.com>",
    "Bill Casarin <bill@casarin.ca> (jb55.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/bitcoinjs/bitcoinjs-server.git"
  }
}
